  "version": "0.2.0",
  "configurations": [
    {
      "name": "domain_visualiser",
      "request": "launch",
      "type": "dart"
    },
    {
      "name": "web",
      "request": "launch",
      "type": "dart",
      "deviceId": "chrome",
      "args": [
        "--web-hostname",
        "localhost",
        "--web-port",
        "5000"],
    },
    {
      "name": "rdt",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_rdt.dart",
    },
    {
      "name": "web&rdt",
      "request": "launch",
      "type": "dart",
      "deviceId": "chrome",
      "program": "lib/main_rdt.dart",
      "args": [
          "--web-hostname",
          "localhost",
          "--web-port",
          "5000"],
    },
  ]
}